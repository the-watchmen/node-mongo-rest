{
  "name": "@watchmen/node-mongo-rest",
  "version": "1.0.2",
  "description": "rest mongo stuff",
  "main": "dist/index.js",
  "repository": "the-watchmen/node-mongo-rest",
  "license": "ISC",
  "scripts": {
    "ava": "cross-env NODE_ENV=auto ava -s test/ava",
    "ava-debug": "cross-env DEBUG=test:*,lib:* NODE_ENV=auto ava -s",
    "cuke": "cross-env NODE_ENV=auto DEBUG=test:*,lib:* cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register test/cuke",
    "cuke-feature": "cross-env DEBUG=test:*,lib:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register",
    "pretest": "xo",
    "test": "npm run ava && npm run cuke",
    "prebuild": "npm run test",
    "build": "babel ./src -d ./dist"
  },
  "dependencies": {
    "@watchmen/geocodr": "^1.1.9",
    "@watchmen/helpr": "^1.0.3",
    "@watchmen/mongo-data": "^1.0.3",
    "@watchmen/mongo-helpr": "^0.9.2",
    "@watchmen/tymer": "^1.2.3",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.16.1",
    "config": "^1.24.0",
    "debug": "^3.1.0",
    "express": "^4.14.1",
    "express-jwt": "^5.1.0",
    "joi": "^13.0.2",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.25"
  },
  "devDependencies": {
    "@watchmen/cuke-helpr": "^3.1.6",
    "@watchmen/mongo-cuke-helpr": "^0.3.6",
    "@watchmen/mongo-test-helpr": "^0.1.9",
    "@watchmen/test-helpr": "^0.3.4",
    "ava": "^0.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-0": "^6.5.0",
    "iso-639-1": "^1.3.0",
    "cross-env": "^5.1.1",
    "cucumber": "^3.1.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "prettier": "^1.7.4",
    "xo": "^0.18.2"
  },
  "xo": {
    "useEslintrc": true
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
