{
  "name": "@watchmen/mongo-rest",
  "version": "2.0.0",
  "description": "rest mongo stuff",
  "main": "dist/index.js",
  "repository": "the-watchmen/node-mongo-rest",
  "license": "ISC",
  "scripts": {
    "ava": "cross-env NODE_ENV=auto ava -s test/ava",
    "ava-debug": "cross-env DEBUG=test:*,lib:* NODE_ENV=auto ava -s",
    "cuke": "cross-env NODE_ENV=auto DEBUG=test:*,lib:* cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register test/cuke",
    "cuke-feature": "cross-env DEBUG=test:*,lib:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register",
    "pretest": "xo",
    "test": "npm run ava && npm run cuke",
    "prebuild": "npm run test",
    "build": "babel ./src -d ./dist",
    "prepare": "install-self-peers -- --ignore-scripts"
  },
  "dependencies": {
    "@watchmen/geocodr": "^1.1.9",
    "@watchmen/helpr": "^2.2.2",
    "@watchmen/mongo-helpr": "^1.0.1",
    "@watchmen/tymer": "^1.2.4",
    "debug": "^3.1.0",
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "@watchmen/mongo-data": ">=2",
    "config": ">=1",
    "express": ">=4"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": ">=1",
    "@watchmen/cuke-helpr": "^3.1.6",
    "@watchmen/mongo-cuke-helpr": "^0.3.6",
    "@watchmen/mongo-test-helpr": "^1.0.0",
    "@watchmen/test-helpr": "^0.3.4",
    "ava": "^0.24.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.1",
    "cucumber": "^3.1.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "express-jwt": "^5.3.0",
    "iso-639-1": "^2.0.3",
    "joi": "^13.0.2",
    "prettier": "^1.7.4",
    "xo": "^0.18.2"
  },
  "xo": {
    "useEslintrc": true
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
