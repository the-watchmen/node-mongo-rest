{
  "name": "@watchmen/mongo-rest",
  "version": "2.1.0",
  "description": "rest mongo stuff",
  "main": "dist/index.js",
  "repository": "the-watchmen/node-mongo-rest",
  "license": "ISC",
  "scripts": {
    "ava": "cross-env NODE_ENV=auto ava -s test/ava",
    "ava-debug": "cross-env DEBUG=test:*,lib:*,dbg:* NODE_ENV=auto ava -s",
    "cuke": "cross-env NODE_ENV=auto DEBUG=test:*,lib:*,dbg:* cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register test/cuke",
    "cuke-feature": "cross-env DEBUG=test:*,lib:*,dbg:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register",
    "pretest": "xo",
    "test": "npm run ava && npm run cuke",
    "prebuild": "npm run test",
    "build": "babel src -d dist",
    "prepare": "install-self-peers -- --ignore-scripts"
  },
  "dependencies": {
    "@watchmen/debug": "1.1.1",
    "@watchmen/geocodr": "1.1.9",
    "@watchmen/helpr": "2.3.1",
    "@watchmen/mongo-helpr": "1.1.0",
    "@watchmen/tymer": "1.2.4",
    "lodash": "4.17.4"
  },
  "peerDependencies": {
    "@watchmen/mongo-data": ">=2",
    "config": ">=1",
    "express": ">=4"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": ">=1",
    "@watchmen/cuke-helpr": "3.2.0",
    "@watchmen/mongo-cuke-helpr": "0.3.7",
    "@watchmen/mongo-test-helpr": "1.0.0",
    "@watchmen/test-helpr": "0.3.4",
    "ava": "0.24.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "1.18.2",
    "cross-env": "5.1.3",
    "cucumber": "3.2.1",
    "eslint": "4.15.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.4.0",
    "express-jwt": "5.3.0",
    "iso-639-1": "2.0.3",
    "joi": "13.1.0",
    "prettier": "1.10.2",
    "xo": "0.18.2"
  },
  "xo": {
    "useEslintrc": true
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
